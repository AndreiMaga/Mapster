<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Mapster.Views.LoadingScreenView"
			 xmlns:vm="using:Mapster.ViewModels"
			 x:DataType="vm:LoadingScreenViewModel">
	<StackPanel HorizontalAlignment="Center"
			VerticalAlignment="Center">
		<ProgressBar Value="{CompiledBinding ProgressValue}" Foreground="Green" ShowProgressText="True" Minimum="0" Maximum="100"></ProgressBar>
	</StackPanel>
</UserControl>
